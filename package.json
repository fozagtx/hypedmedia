{
  "name": "rayban-metadata",
  "version": "1.0.0",
  "description": "Add Meta Ray-Ban smart glasses metadata to video files",
  "main": "dist/index.js",
  "bin": {
    "rayban-meta": "dist/cli.js",
    "rayban": "./rayban-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node dist/index.js",
    "rayban": "node dist/cli.js",
    "dev:cli": "ts-node src/cli.ts",
    "dev:start": "npm run build && npm run rayban",
    "setup": "npm install && npm run build",
    "prepare": "npm run build",
    "postinstall": "npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "meta",
    "rayban",
    "metadata",
    "video",
    "exif",
    "smart-glasses",
    "video-processing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "exiftool-vendored": "^18.5.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/progress": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
